{
    "name": "multidraw",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "server",
        "client"
    ],
    "scripts": {
        "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=client\"",
        "build": "npm run build --workspace=server && npm run build --workspace=client && npm run db:generate --workspace=server",
        "start": "npm start --workspace=server",
        "postinstall": "npm run db:generate --workspace=server"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}